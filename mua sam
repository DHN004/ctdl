#include <stdio.h>
#include <stdlib.h>

int cmp(const void *a, const void *b) {
    return (*(int*)a - *(int*)b);
}

int main() {
    int n;
    long long m;
    int a[100000];

    scanf("%d %lld", &n, &m);
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    // Sắp xếp giá tăng dần
    qsort(a, n, sizeof(int), cmp);

    int count = 0;
    for (int i = 0; i < n; i++) {
        if (m >= a[i]) {
            m -= a[i];
            count++;
        } else {
            break;
        }
    }

    printf("%d", count);
    return 0;
}
