#include <stdio.h>
#include <limits.h>

int main() {
    int n;
    scanf("%d", &n);

    int a;
    int max1 = INT_MIN, max2 = INT_MIN, max3 = INT_MIN;

    for (int i = 0; i < n; i++) {
        scanf("%d", &a);

        if (a > max1) {
            max3 = max2;
            max2 = max1;
            max1 = a;
        }
        else if (a > max2) {
            max3 = max2;
            max2 = a;
        }
        else if (a > max3) {
            max3 = a;
        }
    }

    // In kết quả
    if (n >= 3) {
        printf("%d %d %d", max1, max2, max3);
    } else if (n == 2) {
        printf("%d %d", max1, max2);
    } else if (n == 1) {
        printf("%d", max1);
    }

    return 0;
}
